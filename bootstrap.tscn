[sub_resource type="GDScript" id="GDScript_145kv"]
script/source = "extends Node

signal loading_finished

@export_dir var path_sequence : Array[String]

var paths : PackedStringArray
var total_resources_to_load := 0

func _ready() -> void:
	AudioServer.set_bus_mute(0, true)
	await get_tree().create_timer(3.0 if OS.has_feature(\"web\") else 1.0).timeout
	get_tree().paused = false
	$world.queue_free()
	await get_tree().create_timer(0.1).timeout
	AudioServer.set_bus_mute.call_deferred(0, false)
	get_tree().change_scene_to_file(\"res://assets/scenes/main_menu.tscn\")
"

